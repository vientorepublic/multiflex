{
  "name": "multiflex",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development CURRENT_GIT_SHA=`git rev-parse --short HEAD` CURRENT_GIT_BRANCH=`git branch --show-current` nest start --watch",
    "start:debug": "NODE_ENV=development CURRENT_GIT_SHA=`git rev-parse --short HEAD` CURRENT_GIT_BRANCH=`git branch --show-current` nest start --debug --watch",
    "start:prod": "NODE_ENV=production CURRENT_GIT_SHA=`git rev-parse --short HEAD` CURRENT_GIT_BRANCH=`git branch --show-current` node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@nestia/core": "^3.8.0",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^3.8.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "mysql2": "^3.10.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typia": "^6.5.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestia/benchmark": "^0.1.2",
    "@nestia/sdk": "^3.8.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nestia": "^5.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  }
